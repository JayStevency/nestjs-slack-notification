{
  "name": "nestjs-slack-notification",
  "version": "1.0.1",
  "description": "NestJS module for Slack notifications with global exception handling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "slack",
    "notification",
    "exception",
    "error"
  ],
  "author": "Jaystevency",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/schedule": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.0"
  },
  "dependencies": {
    "@nestjs/config": "^4.0.0",
    "axios": "^1.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^@src/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}